       this.load.tilemap('level1', 'assets/tilemaps/level1.json', null, Phaser.Tilemap.TILED_JSON);
    this.load.image('gameTiles', 'assets/images/tiles.png');
    }
    
   
    
   function create() {
   	   
   }
    
    function update() {
    	
    }
    
};
